/empty-encoding [
	/.notdef
	0 1 7 {
		1 exch bitshift
		copy
	} for
] def


% Создать пустой массив кодировки
/create-encoding {
	256 array
	0 1 255 
	{
		1 index
		exch
		/.notdef
		put
	}
	for
} def

%[arr]
/dup-array {
	dup
	%[arr, arr]
	dup length array
	%[arr, newarr]
	copy
} def

%[arr1, arr2]
/merge-encoding {
	0 dict begin
		/from exch def
		/into exch def
		/len into length def
		
		0 1 len {
			/i exch def
			
			from i get not
			/.notdef eq
			{
				into i from i get put
			}
			if
			
		}for
		
		result
	end
} def
%[new arr]

/create-code-pair {
	2 dict
	dup
	begin
		/Encoding create-encoding def
		/CharProcs <<
			/.notdef {}
		>> def
	end
}

