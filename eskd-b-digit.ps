%Глифы цифры ЕСКД Б
/eskd-b-digit <<
		/DigitZero {
			5 upperceilbegin
				0 0 moveto
				2 right top gliphO
			end
		}
		/DigitOne {
			3 upperfloorbegin
				1 setlinejoin
				0 top 2 sub moveto
				right top lineto
				right 0 lineto
			end
		}
		/DigitTwo {
			5 upperceilbegin
				right 0 moveto
				0 0 lineto
				right top 2 sub lineto
				2 -1 1 arcty
				0 2 xlinearcdown
			end
		}	
		/DigitThree {
			5 upperfloorbegin
				0 0 moveto
				right 2 xlinearcup
				middle 2 ylinearcleft
				1 middle moveto
				right 1.5 xlinearcup
				top 1.5 ylinearcleft
				0 top lineto
			end
		}
		/DigitFour {
			5 upperceilbegin
				right top moveto
				0 2 lineto
				right 1 add 2 lineto
				right 0 moveto
				right middle lineto
			end
		}
		/DigitFive {
			5 upperfloorbegin
				0 0 moveto
				right 2 xlinearcup
				middle 2 ylinearcleft
				0 middle lineto
				0 top lineto
				right top lineto
			end
		}
		/DigitSix {
			5 upperfloorbegin
				right 1 sub top moveto
				0 3 xlinearcdown
				0 1 ylinearcright
				right 1 xlinearcup
				middle 1 ylinearcleft
				0 middle lineto
			end
		}
		/DigitSeven {
			5 upperceilbegin
				1 0 moveto
				right top lineto
				0 top lineto
				0 -1 rlineto
			end
		}
		/DigitEight {
			5 upperfloorbegin
				center middle moveto
				right 2 xlinearcup
				top 2 ylinearcleft
				0 2 xlinearcdown
				middle 2 ylinearcright
				right 2 xlinearcdown
				0 1.5 ylinearcleft
				0 1.5 xlinearcup
				middle 2 ylinearcright
			end
		}	
		/DigitNine {
			5 upperceilbegin
				1 0 moveto
				right 3 xlinearcup
				top 1 ylinearcleft
				0 1 xlinearcdown
				middle 1 ylinearcright
				right middle lineto
			end
		}

>> def