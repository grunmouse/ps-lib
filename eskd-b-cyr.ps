%Набор кириллических глифов ЕСКД Б

/eskd-b-cyrillic <<
	/CyrillicCapitalLetterA {
		7 setconfig uppertop
		/middle 2 def
		1 setlinejoin
		A moveto
		N lineto
		D lineto
		A N 2 2crossh moveto
		N D 2 2crossh lineto
	}
	/CyrillicCapitalLetterBe {
		6 setconfig uppertop middleceil
		/center right 1 sub def
		B moveto
		A lineto
		1 O gliphC
		B moveto
		right 1 sub 0 rlineto
	}
	/CyrillicCapitalLetterVe {
		6 setconfig uppertop middleceil
		/center right 1 sub def
		B moveto
		A lineto
		1 O gliphC
		B moveto
		N Q 1 arct
		Q M 2 arct
		
	}
	/CyrillicCapitalLetterGhe {
		4 setconfig uppertop
		/right inc
		A moveto
		B lineto
		C lineto
		
	}
	/CyrillicCapitalLetterDe {
		5 setconfig uppertop
		-1 -1 moveto
		-1 0 lineto
		right 1 add 0 lineto
		0 -1 rlineto
		0 0 moveto
		1 setlinejoin 
		1  top lineto
		right top  lineto
		0 setlinejoin 
		right 0 lineto
		
	}
	/CyrillicCapitalLetterIe {
		5 setconfig uppertop middleceil
		right 0 moveto
		0 0 lineto
		0 top lineto
		right top lineto
		0 middle moveto
		3 0 rlineto
		
	}
	/CyrillicCapitalLetterIo {
		CyrillicCapitalLetterIe
		stroke
		subglyph /uml get exec
	}
	/CyrillicCapitalLetterZhe {
		8 setconfig uppertop middleceil
			
		P moveto N lineto
		
		Q moveto C lineto
		Q moveto B lineto
		
		Q C center inc 2crossv moveto
		D lineto
		
		Q B center dec 2crossv moveto
		A lineto
	}
	/CyrillicCapitalLetterZe {
		5 setconfig uppertop middleceil
		/center dec
		1 setlinecap
		0 1 moveto
		A D 1 arct
		D O 2 arct
		O Q 2 arct
		Q moveto
		O C 2 arct
		C B 1 arct
		B A 1 arct
	}
	/CyrillicCapitalLetterI {
		6 setconfig uppertop
		1 setlinejoin 
		2 setlinecap
		B moveto
		D gliphN
		
	}
	/CyrillicCapitalLetterShortI {
		CyrillicCapitalLetterI
		stroke
		subglyph /breve get exec
		
	}
	/CyrillicCapitalLetterKa {
		5 setconfig uppertop middleceil
		A moveto B lineto
		M moveto
		C lineto
		M C 1 2crossv moveto
		D lineto
		
	}
	/CyrillicCapitalLetterEl {
		6 setconfig uppertop
		0 0 moveto
		2 top lineto
		right top lineto
		right 0 lineto
	}
	/CyrillicCapitalLetterEm {
		7 setconfig uppertop middleceil
		1 setlinejoin 
		A moveto
		B lineto
		Q lineto
		C lineto
		D lineto
	}
	/CyrillicCapitalLetterEn {
		6 setconfig uppertop middleceil
		A moveto
		C middle gliphH
	}
	/CyrillicCapitalLetterO {
		6 setconfig uppertop middleceil
		A moveto
		2 C gliphO
	}
	/CyrillicCapitalLetterPe {
		6 setconfig uppertop middleceil
		0 0 moveto
		0 top lineto
		right top lineto
		right 0 lineto
	}
	/CyrillicCapitalLetterEr {
		6 setconfig uppertop middlefloor
			0 0 moveto
			0 top lineto
			0 middle moveto
			1 right top gliphC
			
		
	}
	/CyrillicCapitalLetterEs {
		6 setconfig uppertop middlefloor
			right top moveto
			2 0 0 gliphC
			
		
	}
	/CyrillicCapitalLetterTe {
		6 setconfig uppertop middlefloor
			center 0 moveto
			0 top rlineto
			0 top moveto
			right 0 rlineto
			
		
	}
	/CyrillicCapitalLetterU {
		6 setconfig uppertop middlefloor
			0 top moveto
			right middle 1 arcYX
			right top moveto
			1 0 2 arcYX
		
	}
	/CyrillicCapitalLetterEf {
		8 setconfig uppertop middlefloor
		center 0 moveto
		0 top rlineto
		0 2 moveto

		2 
		right 
		top 2 sub 
		gliphO
			
		
	}
	/CyrillicCapitalLetterHa {
		7 setconfig uppertop middlefloor
		A moveto C gliphX
		
	}
	/CyrillicCapitalLetterTse {
		6 setconfig uppertop middlefloor
		0 top  moveto
		0 0 lineto
		g 0 lineto
		0 -1 rlineto
		right top moveto
		right 0 lineto
			
		
	}
	/CyrillicCapitalLetterChe {
		6 setconfig uppertop middlefloor
		0 top moveto
		right middle 1 arcYX
		right top moveto
		right 0 lineto
			
		
	}
	/CyrillicCapitalLetterSha {
		8 setconfig uppertop middlefloor
		B moveto
		A lineto
		D lineto
		C lineto
		P moveto
		N lineto
	}
	/CyrillicCapitalLetterShcha {
		8 setconfig uppertop
		B moveto
		A lineto
		D lineto
		C lineto
		P moveto
		N lineto
		right inc 0 moveto
		0 -1 rlineto
	}
	/CyrillicCapitalLetterHardSign {
		6 setconfig uppertop middleceil
		A moveto
		1 O gliphC
		A moveto
		B lineto
		-2 0 rlineto
			
		
	}
	/CyrillicCapitalLetterYeru {
		7 setconfig uppertop middleceil
		0 0 moveto
		1 4 middle gliphC
		0 0 moveto
		0 top lineto
		right top moveto
		right 0 lineto
			
				
	}
	/CyrillicCapitalLetterSoftSign {
		6 setconfig uppertop middleceil
			0 0 moveto
			1 right middle gliphC
			0 0 moveto
			0 top lineto
		
	}
	/CyrillicCapitalLetterE {
		6 setconfig uppertop middleceil
			0 0 moveto
			1 right top gliphC
			right middle moveto
			center middle lineto
			
		
	}
	/CyrillicCapitalLetterYu {
		7 setconfig uppertop middleceil
			0 0 moveto 0 top lineto
			0 middle moveto	2 0 rlineto
			2 0 moveto
			2 right top gliphO
		
	}
	/CyrillicCapitalLetterYa {
		6 setconfig uppertop middlefloor
			1 setlinecap
			right 0 moveto
			right top lineto
			
			1 0 middle gliphC
			center middle moveto
			
			0 0 lineto
			
		
	}
	/CyrillicSmallLetterA {
		5 setconfig lowertop middleceil
			right top moveto
				0 0 beginABCD
				D C 1.5 arct
				C B 1.5 arct
				B A 1 arct
				/M currentpoint 2def
				A lineto
				stroke
				1 setlinecap
				M moveto
				1 -1 rlineto
			end
		
	}
	/CyrillicSmallLetterBe {
		5 setconfig lowertop middleceil
			1 setlinejoin
			right 1 sub dup top moveto
			neg 3 rlineto
			right 0 rlineto
			0 0 moveto
			1 right top gliphO
			
		
	}
	/CyrillicSmallLetterVe {
		5 setconfig lowertop middleceil
			0 top moveto
			1 right 0 gliphOa
			currentpoint 3 3 2add beginABCD
				/O 1 top 2def
				B C 1 arct
				C O 1 arct
				O lineto
			end
		
	}
	/CyrillicSmallLetterGhe {
		4 setconfig lowertop middleceil
			1 setlinejoin
			1 setlinecap
			0 dict begin
				/d 1 3 2 4 2lineYX def %накллонная средняя линия
				/A 0 top 2def
				/B right top 2def
				/C right dup d 2def
				/D 0 dup d 2def
				/E 0 0 2def
				/F right 0.5 add 0 2def
				/H F 0 1 2add 2def
				
				A 0 -1 2add moveto
				%A lineto
				A B 1 arct
				%B lineto
				B C 1 arct
				C lineto
				%C D 1 arct
				%D lineto
				D E 
				D E max-arct 2 div
				%1.5 
				arct
				%E lineto
				E F 1.5 arct
				%F lineto
				F H 1 arct
				H lineto
			end
		
	}
	/CyrillicSmallLetterDe {
		5 setconfig lowertop middleceil
			0 top 3 add moveto
			right 0 beginABCD
				0 top setMNOPQ
				1 0 rmoveto
				1 [D C B M O] many-arct
				lineto
			end
		
	}
	/CyrillicSmallLetterIe {
		5 setconfig lowertop middleceil
			right 0 moveto
			0 top beginABCD
				0 middle setMNOPQ
				A moveto
				1 [D C B M O] many-arct
				lineto
			end
		
	}
	/CyrillicSmallLetterIo {
		CharProcs /CyrillicSmallLetterIe get exec
		stroke
		subglyph /uml get exec
	}
	/CyrillicSmallLetterZhe {
		7 setconfig lowertop middleceil
			center  0  moveto
			center  top lineto
			
			center  middle moveto
			right top lineto

			center  middle moveto
			0  top lineto

			center  middle moveto 0.5 0.5 rmoveto
			right 0  lineto
			center  middle moveto -0.5 0.5 rmoveto
			0  0  lineto
			
		
	}
	/CyrillicSmallLetterZe {
		4 setconfig lowertop middleceil
			1 setlinecap
			0 0 moveto
			right top beginABCD
				center middle setMNOPQ
				0 1 moveto
				A D 1 arct
				D O 1 arct
				O Q 1 arct
				-1 0 rlineto
				1 0 rmoveto
				O C 1 arct
				C B 1 arct
				B A 1 arct
			end			
			
		
	}
	/CyrillicSmallLetterI {
		5 setconfig lowertop middleceil
			0 top moveto
			right 0 1 arcYX
			right top lineto
			
		
	}
	/CyrillicSmallLetterShortI {
		CharProcs /CyrillicSmallLetterI get exec
