			0 top moveto
			0 1 ylinearcright
			right 0 lineto
			right top moveto
			-3 2 ylinearcleft
			1 -3 lineto
			
		end
	}
	/CyrillicSmallLetterEf {
		7 lowerbegin
			0 0 moveto
			1 right top glifo
			center top moveto 0 2 rmoveto 
