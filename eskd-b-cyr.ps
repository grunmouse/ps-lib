		5 setconfig lowertop middleceil
			0 top moveto
			right middle 1 arcYX
			right top moveto
			right 0 lineto
			
		
	}
	/CyrillicSmallLetterSha {
		7 setconfig lowertop middleceil
			0 top moveto
			right 0 1 arcYX
			right top lineto
			center 0 moveto
