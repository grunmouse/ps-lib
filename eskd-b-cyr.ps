%Набор кириллических глифов ЕСКД Б

/eskd-b-cyrillic <<
	/CyrillicCapitalLetterA {
		7 setconfig uppertop middlefloor
			1 setlinejoin
			0 0 moveto
			center top lineto
			right 0 lineto
			center top div 2 mul dup 2 moveto
			right exch sub 2 lineto
			
		
	}
	/CyrillicCapitalLetterBe {
		6 setconfig uppertop middleceil
			0 top moveto
			0 0 lineto
			1 right middle gliphC
			0 top moveto
			right 1 sub 0 rlineto
			
		
	}
	/CyrillicCapitalLetterVe {
		6 setconfig uppertop middleceil
			0 top moveto
			0 0 lineto
			1 right middle gliphC
			0 top moveto
			right 1 sub middle beginABCD
				D C 1 arct
				C B 2 arct
			end
		
	}
	/CyrillicCapitalLetterGhe {
		4 setconfig uppertop middleceil
			0 0 moveto
			0 top lineto
			right 1 add top lineto
		
	}
	/CyrillicCapitalLetterDe {
		5 setconfig uppertop middleceil
			-1 -1 moveto
			-1 0 lineto
			right 1 add 0 lineto
			0 -1 rlineto
			0 0 moveto
			1 setlinejoin 
			1  top lineto
			right top  lineto
			0 setlinejoin 
			right 0 lineto
		
	}
	/CyrillicCapitalLetterIe {
		5 setconfig uppertop middleceil
			right 0 moveto
			0 0 lineto
			0 top lineto
			right top lineto
			0 middle moveto
			3 0 rlineto
		
	}
	/CyrillicCapitalLetterIo {
		CharProcs /CyrillicCapitalLetterIe get exec
		stroke
		
		subglyph /uml get exec
				
	}
	/CyrillicCapitalLetterZhe {
		8 setconfig uppertop middleceil
			
			center 0 moveto
			center top lineto
			
			center middle moveto
			right top lineto
			center middle moveto
			0 top lineto
			
			center middle moveto 
			top middle sub center div dup
			1 exch rmoveto
			right 0 lineto
			
			center middle moveto
			-1 exch rmoveto
			0 0 lineto
			
		
	}
	/CyrillicCapitalLetterZe {
		5 setconfig uppertop middleceil
			1 setlinecap
			0 0 moveto
			right top beginABCD
				center middle setMNOPQ
				0 1 moveto
				A D 1 arct
				D O 2 arct
				O Q 2 arct
				-1 0 rlineto
				1 0 rmoveto
				O C 2 arct
				C B 1 arct
				B A 1 arct
			end
		
	}
	/CyrillicCapitalLetterI {
		6 setconfig uppertop middleceil
			1 setlinejoin 
			0 top moveto
			right 0 gliphN
		
	}
	/CyrillicCapitalLetterShortI {
		2 setlinecap
		CharProcs /CyrillicCapitalLetterI get exec
		stroke
		
			subglyph /breve get exec
		
	}
