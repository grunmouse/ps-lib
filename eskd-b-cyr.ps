	/CyrillicSmallLetterI {
		5 lowerbegin
			0 top moveto
			0 1 ylinearcright
			right 0 lineto
			right top lineto
			
		end
	}
	/CyrillicSmallLetterShortI {
		CharProcs /CyrillicSmallLetterI get exec
		stroke
		overbegin
			subglyph /breve get exec
		end		
	}
	/CyrillicSmallLetterKa {
		5 lowerbegin
			0 0 moveto
			0 top lineto
			0 middle 1 sub moveto
			right top lineto
			0 middle moveto 2 middle right div 2 mul 1 sub rmoveto
			right 0 lineto
			
		end
	}
	/CyrillicSmallLetterEl {
		5 lowerbegin
			1 setlinejoin
			0 0 moveto
			2 top lineto
