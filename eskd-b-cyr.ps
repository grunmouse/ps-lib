	/CyrillicSmallLetterA {
		5 setconfig lowertop middleceil
			right top moveto
				0 0 beginABCD
				D C 1.5 arct
				C B 1.5 arct
				B A 1 arct
				/M currentpoint 2def
				A lineto
				stroke
				1 setlinecap
				M moveto
				1 -1 rlineto
			end
		
	}
	/CyrillicSmallLetterBe {
		5 setconfig lowertop middleceil
			1 setlinejoin
			right 1 sub dup top moveto
			neg 3 rlineto
			right 0 rlineto
			0 0 moveto
			1 right top gliphO
			
		
	}
	/CyrillicSmallLetterVe {
		5 setconfig lowertop middleceil
			0 top moveto
			1 right 0 gliphOa
			currentpoint 3 3 2add beginABCD
				/O 1 top 2def
				B C 1 arct
				C O 1 arct
				O lineto
			end
		
	}
	/CyrillicSmallLetterGhe {
		4 setconfig lowertop middleceil
			1 setlinejoin
			1 setlinecap
			0 dict begin
				/d 1 3 2 4 2lineYX def %накллонная средняя линия
				/A 0 top 2def
				/B right top 2def
				/C right dup d 2def
				/D 0 dup d 2def
				/E 0 0 2def
				/F right 0.5 add 0 2def
				/H F 0 1 2add 2def
				
				A 0 -1 2add moveto
				%A lineto
				A B 1 arct
				%B lineto
				B C 1 arct
				C lineto
				%C D 1 arct
				%D lineto
				D E 
				D E max-arct 2 div
				%1.5 
				arct
				%E lineto
				E F 1.5 arct
				%F lineto
				F H 1 arct
				H lineto
			end
		
	}
	/CyrillicSmallLetterDe {
		5 setconfig lowertop middleceil
			0 top 3 add moveto
			right 0 beginABCD
				0 top setMNOPQ
				1 0 rmoveto
